{
  "manifest_version": 3,
  "name": "Auto Reward Sender",
  "version": "1.0",
  "description": "Automatically sends rewards to a specified address.",
  "permissions": ["storage", "alarms", "notifications"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "background": {
    "service_worker": "index.html"
  }
}
